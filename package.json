{
  "name": "stinsoncoin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "hardhat compile",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "test": "hardhat test",
    "local-testnet": "hardhat node",

    "deploy:local-nartCoin": "hardhat run scripts/deployNartCoin.ts",
    "deploy:goerli-nartCoin": "hardhat run scripts/deployNartCoin.ts --network goerli",
    "deploy:hardhat-greeter": "hardhat deploy:Greeter",
    "deploy:hardhat-nartCoin" : "hardhat deploy:NartCoin",

    "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"test/**/*.ts\" && yarn typechain",
    "clean": "shx rm -rf ./artifacts ./cache ./coverage ./types ./coverage.json && yarn typechain",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",

    "commit": "git-cz",
    "lint": "yarn lint:sol && yarn lint:ts && yarn prettier:check",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --ignore-path ./.eslintignore --ext .js,.ts .",
    "postinstall": "husky install && DOTENV_CONFIG_PATH=./.env.example yarn typechain",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier:check": "prettier --check \"**/*.{js,json,md,sol,ts,yml}\"",
    "prettier:write": "prettier --write \"**/*.{js,json,md,sol,ts,yml}\""
  },
  "keywords": [
    "blockchain",
    "ethers",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity",
    "template",
    "typescript",
    "typechain"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.7",
    "@nomicfoundation/hardhat-toolbox": "^2.0.1",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.5",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/chai": "^4.3.4",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "chai": "^4.3.7",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "ethereum-waffle": "^4.0.9",
    "ethers": "^5.7.2",
    "fs-extra": "^11.1.0",
    "hardhat": "^2.12.6",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "lodash": "^4.17.21",
    "pinst": "^3.0.0",
    "prettier": "^2.8.3",
    "prettier-plugin-solidity": "^1.1.1",
    "shx": "^0.3.4",
    "solhint": "^3.3.8",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.2",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.5"
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "/contracts"
  ],
  "publishConfig": {
    "access": "public"
  }
}
